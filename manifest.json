{
  "name": "CDN offline proxy/cache",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon-on-48.png",
    "128": "icons/icon-on-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-off-19.png",
      "38": "icons/icon-off-38.png"
    },
    "default_title": "CDN offline proxy/cache"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "webRequest", "webRequestBlocking",
    "*://*/*"
  ]
}